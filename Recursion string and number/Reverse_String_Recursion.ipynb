    "def reverse_string(a,i=0):\n",
    "    ''' String Reverse using recursion '''\n",
    "    if i < len(a)-1:\n",
    "        reverse_string(a,i+1)\n",
    "    print(a[i],end=\"\")\n",
    "    \n",
    "reverse_string('Hello World')"
